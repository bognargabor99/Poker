<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PokerCardView">
        <attr name="isUpside" format="boolean" />
        <attr name="value" format="integer" />
        <attr name="symbol" format="enum">
            <enum name="heart" value="0" />
            <enum name="spade" value="1" />
            <enum name="diamond" value="2" />
            <enum name="club" value="3" />
        </attr>
    </declare-styleable>
</resources>